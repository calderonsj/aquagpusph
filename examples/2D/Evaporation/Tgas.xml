<sphInput>
    <Variables>
	<Variable name="Ltot" type="float" value="0.1" />
	<Variable name="Htot" type="float" value="0.3" />
	<!--Variable name="Tsolid" type="float" value="91.8" /-->
	<Variable name="q" type="float" value="60.0" />    
	<Variable name="Tgas" type="float" value="90.0" />
    <Variable name="Tsat" type="float" value="90.0" />
    <Variable name="pgas" type="float" value="0.0" />
    <Variable name="pSat" type="float" value="0.0" />
	<Variable name="kgas" type="float" value="0.5" />
    <Variable name="kliq" type="float" value="0.5" />
	<Variable name="mgas" type="float" value="0.0" />
	<Variable name="rhog" type="float" value="1" />
	<Variable name="hg" type="float" value="0.15" />
	<Variable name="cvgas" type="float" value="900" />
	<Variable name="cvliq" type="float" value="2000" />
    <Variable name="actant" type="float" value="90.0" />
    <Variable name="bctant" type="float" value="11.308" />     
    <Variable name="alphac" type="float" value="0.0"/>
    <Variable name="alphav" type="float" value="0.025"/>
	<Variable name="dmgas" type="float" value="0.0" /> 
    <Variable name="mint" type="float*" length="N" /> 
	<Variable name="Mint" type="float" value="0.0" /> 
	<Variable name="nfs" type="int*" length="N" />
	<Variable name="nFS" type="int" value="0" />
    <Variable name="mref" type="float" value="0.001497" />       
    <Variable name="hint" type="float" value="h"/>    
    <Variable name="Ru" type="float" value="8.314" />
    <Variable name="molargas" type="float" value="0.0249445" />
	<Variable name="vol" type="float*" length="N" />
	<Variable name="Vol" type="float" value="0.0" />
    </Variables>

  <Tools>
    <Tool entry_point="vol" action="insert" before="t = t + dt" name="vol" path="Volume.cl" type="kernel"/>
    <Tool entry_point="Vol" action="insert" before="t = t + dt" name="Vol" type="reduction" in="vol" out="Vol" null="0.f">
       c = a + b;
    </Tool>
    <Tool entry_point="mint" action="insert" before="corrector" name="mint" path="massint.cl" type="kernel"/>
    <Tool entry_point="Mint" action="insert" before="corrector" name="Mint" type="reduction" in="mint" out="Mint" null="0.f">
       c = a + b;
    </Tool> 
    <Tool entry_point="nfs" action="insert" before="corrector" name="nfs" path="nFS.cl" type="kernel"/>
    <Tool entry_point="nFS" action="insert" before="corrector" name="nFS" type="reduction" in="nfs" out="nFS" null="0">
       c = a + b;
    </Tool> 
    <Tool action="insert" before="corrector" name="Tgas" path="Tgas.py" type="python"/>
    <Tool entry_point="mint2" action="insert" after="Tgas" name="Massamp" path="massamp.cl" type="kernel" />
  </Tools>
</sphInput>